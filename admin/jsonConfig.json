{
    "i18n": true,
    "type": "tabs",
    "iconPosition": "top",
    "items": {
        "_general": {
            "type": "panel",
            "label": "General settings",
            "icon": "data:image/svg+xml;base64,",
            "items": {
                "host": {
                    "newLine": true,
                    "type": "text",
                    "sm": 10,
                    "md": 4,
                    "lg": 4,
                    "label": "host"
                },
                "port": {
                    "type": "port",
                    "sm": 2,
                    "md": 4,
                    "lg": 4,
                    "label": "port"
                },
                "mqttVersion": {
                    "type": "select",
                    "options": [
                        {
                            "label": "3",
                            "value": 3
                        },
                        {
                            "label": "4",
                            "value": 4
                        },
                        {
                            "label": "5",
                            "value": 5
                        }
                    ],
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "MQTT Version"
                },
                "websocket": {
                    "newLine": true,
                    "type": "checkbox",
                    "sm": 4,
                    "md": 4,
                    "lg": 4,
                    "label": "Use websockets"
                },
                "ssl": {
                    "type": "checkbox",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "ssl"
                },
                "rejectUnauthorized": {
                    "hidden": "!data.ssl",
                    "type": "checkbox",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "rejectUnauthorized"
                },
                "clientId": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Client ID"
                },
                "username": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "username"
                },
                "password": {
                    "type": "password",
                    "repeat": false,
                    "visible": true,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "password"
                },
                "_headerCustomOptions": {
                    "newLine": true,
                    "type": "header",
                    "size": 2,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "text": "Custom options"
                },
                "onConnectTopic": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "on connect topic",
                    "validator": "!data.onConnectTopic.startsWith('/')",
                    "validatorErrorText": "Topic must not start with /",
                    "validatorNoSaveOnError": true
                },
                "onConnectMessage": {
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "on connect message"
                },
                "onDisconnectTopic": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "on disconnect topic",
                    "validator": "!data.onDisconnectTopic.startsWith('/')",
                    "validatorErrorText": "Topic must not start with /",
                    "validatorNoSaveOnError": true
                },
                "onDisconnectMessage": {
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "on disconnect message"
                },
                "lastWillTopic": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "last will topic",
                    "validator": "!data.lastWillTopic.startsWith('/')",
                    "validatorErrorText": "Topic must not start with /",
                    "validatorNoSaveOnError": true
                },
                "lastWillMessage": {
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "last will message"
                },
                "inbox": {
                    "newLine": true,
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "prefix for subscribing topics",
                    "validator": "!data.inbox.endsWith('/')",
                    "validatorErrorText": "Topic must not end with /",
                    "validatorNoSaveOnError": true
                },
                "outbox": {
                    "type": "text",
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "prefix for publishing topics",
                    "validator": "!data.outbox.endsWith('/')",
                    "validatorErrorText": "Topic must not end with /",
                    "validatorNoSaveOnError": true
                },
                "subscriptions": {
                    "type": "chips",
                    "label": "additional subscriptions",
                    "delimiter": ",",
                    "sm": 12,
                    "md": 12,
                    "lg": 12
                }
            }
        }
    }
}